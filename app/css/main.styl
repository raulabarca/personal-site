ul li
  list-style: none

a:hover
  text-decoration: none

.main
  width: 100%;
  max-height: 600px;
  height: auto !important;
  position: absolute;
  top:0;
  left:0;
  overflow: hidden
  vertical-align: baseline
  margin: 0
  padding: 0
  border: 0
  background-image: url(/img/background.png)
  background-position: top center no-repeat
  background-size: 100%
  background-repeat: no-repeat

#portfolio
  position: absolute
  bottom: -300px
  width: 100%
  margin-bottom: 20px
  margin-top: 300px

  @media screen and (max-width: 768px)
    position: relative

#portfolio ul li
  float: left
  text-align: center

#portfolio ul
  padding-left: 0%

.exemple
  padding: 0 0 5px
  margin: 60px
  width: 250px
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1)

  @media screen and (max-width: 768px)
    margin: 10px

#down
  margin-left: -27px
  text-align: center

h1
  font-size: 50px

.see-more
  position: absolute
  bottom: 35px

.pointer
  cursor: pointer

.logo
  text-align: center
  color: #fff
  padding-top: 15%
  padding-bottom:200px

.social ul li
  float: left
  margin-right: 23px;
  text-align: center

.social ul
  margin: auto

.social
  height: 30px
  margin: auto
  width: 150px
  position: fixed
  right: 15px
  top: 5px
  color: #fff

.mas
  float: right
  position: fixed
  top: 5px
  right: 5px
  color: #fff

#footer
  height: 50px
  text-align: center
  margin-top: 200px
  clear:both
